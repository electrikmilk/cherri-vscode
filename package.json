{
  "name": "cherri-vscode-extension",
  "displayName": "Cherri for VSCode",
  "description": "Provides support for the Cherri Siri Shortcuts programming language. Provides Syntax highlighting and file icon. Originally by erenyenigul.",
  "version": "0.0.3",
  "publisher": "electrikmilk",
  "repository": {
    "url": "https://github.com/electrikmilk/cherri-vscode-highlight"
  },
  "icon": "cherri_icon.png",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cherri",
      "aliases": ["Cherri", "cherri"],
      "extensions": [".cherri"],
      "configuration": "./language-configuration.json",
      "icon": {
         "light": "./cherri_icon.png",
         "dark": "./cherri_icon.png"
       }
    }],
    "grammars": [{
      "language": "cherri",
      "scopeName": "source.cherri",
      "path": "./syntaxes/cherri.tmLanguage.json"
    }]
  }
}
